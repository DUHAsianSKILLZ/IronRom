<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.provider.emergencymode" >    <home>        <!-- Permission[wxyz] -->        <!--             w : Always enabled.             x : Use AlarmManager.             y : Use mobile data when screen off.             z : Use default App permission, Don't kill if turn off screen.        -->        <!-- SBrowser -->        <appshortcut            launcher:className="com.sec.android.app.sbrowser.SBrowserMainActivity"            launcher:packageName="com.sec.android.app.sbrowser"            launcher:permission="1001" />        <!-- Hangouts -->        <appshortcut            launcher:className="com.google.android.talk.SigningInActivity"            launcher:packageName="com.google.android.talk"             launcher:permission="0000" />        <!-- Google+ -->        <appshortcut            launcher:className="com.google.android.apps.plus.phone.HomeActivity"            launcher:packageName="com.google.android.apps.plus"            launcher:permission="0000" />        <!-- Maps -->        <appshortcut            launcher:className="com.google.android.maps.MapsActivity"            launcher:packageName="com.google.android.apps.maps"             launcher:permission="0000" />        <!-- G Mails -->        <appshortcut            launcher:className="com.google.android.gm.ConversationListActivityGmail"            launcher:packageName="com.google.android.gm"             launcher:permission="0000" />        <!-- Youtube -->        <appshortcut            launcher:className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity"            launcher:packageName="com.google.android.youtube"             launcher:permission="0000" />         <!-- Calculator -->        <appshortcut            launcher:className="com.sec.android.app.popupcalculator.Calculator"            launcher:packageName="com.sec.android.app.popupcalculator"            launcher:permission="0000" />                <!-- Clock -->        <appshortcut            launcher:className="com.sec.android.app.clockpackage.TabletClockPackage"            launcher:packageName="com.sec.android.app.clockpackage"            launcher:permission="0101" />                <!-- EvenNote -->        <appshortcut            launcher:className="com.evernote.ui.HomeActivity"            launcher:packageName="com.evernote"             launcher:permission="0000" />                <!-- S Voice -->        <appshortcut            launcher:className="com.vlingo.midas.gui.ConversationActivity"            launcher:packageName="com.vlingo.midas"            launcher:permission="0101" />    </home></favorites>